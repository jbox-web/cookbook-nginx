<%- @config.each do |key, value| %>
<%- if value.is_a?(String) || value.is_a?(Integer) -%>
<%= key %> <%= value %>;
<%- elsif value.is_a?(Hash) -%>
<%= key %> {
  <%- includes = value.select { |k, v| k == 'include' } -%>
  <%- value.reject { |k, v| k == 'include' }.each do |sub_key, sub_value| -%>
  <%- if sub_value.is_a?(Array) -%>
    <%- sub_value.each do |v| -%>
  <%= sub_key %> <%= v %>;
    <%- end -%>
  <%- else -%>
  <%= sub_key %> <%= sub_value %>;
  <%- end -%>
  <%- end -%>
  <%- includes.each do |k, v| -%>
    <%- v.each do |inc| -%>
  include <%= inc %>;
    <%- end -%>
  <%- end -%>
}
<%- end -%>
<%- end -%>
